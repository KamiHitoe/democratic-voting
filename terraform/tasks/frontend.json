[
  {
    "name": "democratic-frontend-container",
    "image": "716145811673.dkr.ecr.ap-northeast-1.amazonaws.com/test-frontend:v0.0.1",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "frontend",
        "awslogs-group": "/ecs/frontend"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "environment": [
      {
        "name": "WORKDIR",
        "value": "/app"
      }
    ]
  }
]